@{

}

<div class="row">
    <div class="col-6">
        <h2 class="text-primary">Kullanıcı Listesi </h2>
    </div>
    <div class="col-6 text-right">
        <a class="btn btn-primary" asp-action="AddUser"><i class="fas fa-plus"></i> &nbsp; Kullanıcı Ekle</a>
    </div>
</div>

<br />
<div class="p-4 border rounded">
    <table id="tblData" class="table table-striped table-bordered" style="width:100%">
        <thead class="thead-dark">
            <tr class="table-info">
            
                <th>Kullanıcı Ad</th>
                <th>Kullanıcı Soyad</th>
                <th>Kullanıcı Mail</th>
                <th>Kullanıcı Doğum</th>
                <th>Kullanıcı Gsm</th>
                <th>Kullanıcı Kontrol</th>
             

            </tr>
        </thead>
    </table>
</div>


@section Scripts{
    @if (TempData["AddStatus"] != null){ <script src="~/js/AddUserPop.js"></script>}
    @if (TempData["UpdateStatus"] != null){ <script src="~/js/UpdateUserPop.js"></script>}
    <script src="~/js/userListAdmin.js"></script> 
}





